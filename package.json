  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lalartu/git-test/issues"
  },
  "homepage": "https://github.com/lalartu/git-test#readme",
  "devDependencies": {
    "lite-server": "^2.5.4",
    "node-sass": "^4.13.1",
    "onchange": "^3.3.0",
    "parallelshell": "^3.0.1"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.1"
  }
}
